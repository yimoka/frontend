{
  "name": "rn-app",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "reset-project": "node ./scripts/reset-project.js",
    "test": "jest --watchAll",
    "lint": "eslint . --fix",
    "lint:check": "eslint .",
    "export": "expo export --output-dir ./dist",
    "build:ios:dev": "node ./scripts/build-ios-dev.js",
    "build:android:dev": "node ./scripts/build-android-dev.js",
    "build:ios:prod": "node ./scripts/build-ios-prod.js",
    "build:android:prod": "node ./scripts/build-android-prod.js",
    "build:ios:prod:local": "node ./scripts/build-ios-prod-local.js",
    "build:android:prod:local": "node ./scripts/build-android-prod-local.js",
    "build:prod": "npm run build:ios:prod && npm run build:android:prod",
    "build:prod:local": "npm run build:ios:prod:local && npm run build:android:prod:local",
    "install:ios": "node ./scripts/install-ios.js",
    "install:android": "node ./scripts/install-android.js",
    "start": "expo start --dev-client --clear",
    "clean": "rm -rf build/ios/* build/android/* build-*.tar.gz *.apk *.aab dist"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.2",
    "@react-navigation/bottom-tabs": "^7.2.0",
    "@react-navigation/native": "^7.0.14",
    "expo": "~52.0.38",
    "expo-blur": "~14.0.3",
    "expo-constants": "~17.0.8",
    "expo-dev-client": "~5.0.14",
    "expo-font": "~13.0.4",
    "expo-haptics": "~14.0.1",
    "expo-linking": "~7.0.5",
    "expo-router": "~4.0.18",
    "expo-splash-screen": "~0.29.22",
    "expo-status-bar": "~2.0.1",
    "expo-symbols": "~0.2.2",
    "expo-system-ui": "~4.0.8",
    "expo-updates": "~0.27.3",
    "expo-web-browser": "~14.0.2",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native": "0.76.7",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-reanimated": "~3.16.1",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.4.0",
    "react-native-web": "~0.19.13",
    "react-native-webview": "13.12.5"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/jest": "^29.5.12",
    "@types/react": "~18.3.12",
    "@types/react-test-renderer": "^18.3.0",
    "eas-cli": "^15.0.15",
    "jest": "^29.2.1",
    "jest-expo": "51.0.2",
    "react-test-renderer": "18.3.1"
  },
  "private": true
}